<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/splash_nave"
    app:startDestination="@id/splash">

    <fragment
        android:id="@+id/onBoardingOneFragment"
        android:name="com.solutionplus.altasherat.features.splash.presention.OnBoardingOneFragment"
        android:label="fragment_on_boarding_one"
        tools:layout="@layout/fragment_on_boarding_one" >
        <action
            android:id="@+id/action_onBoardingOneFragment_to_onBoardingTwoFragment"
            app:destination="@id/onBoardingTwoFragment" />
    </fragment>
    <fragment
        android:id="@+id/onBoardingTwoFragment"
        android:name="com.solutionplus.altasherat.features.splash.presention.OnBoardingTwoFragment"
        android:label="fragment_on_boarding_two"
        tools:layout="@layout/fragment_on_boarding_two" >
        <action
            android:id="@+id/action_onBoardingTwoFragment_to_onBoardingThreeFragment"
            app:destination="@id/onBoardingThreeFragment" />
    </fragment>
    <fragment
        android:id="@+id/onBoardingThreeFragment"
        android:name="com.solutionplus.altasherat.features.splash.presention.OnBoardingThreeFragment"
        android:label="fragment_on_boarding_three"
        tools:layout="@layout/fragment_on_boarding_three" />
    <fragment
        android:id="@+id/splash"
        android:name="com.solutionplus.altasherat.features.splash.presention.SplashFragment"
        android:label="fragment_splach"
        tools:layout="@layout/fragment_splach" >
        <action
            android:id="@+id/action_splashFragment_to_onBoardingOneFragment"
            app:destination="@id/onBoardingOneFragment"
            app:popUpTo="@id/onBoardingOneFragment" />
    </fragment>
</navigation>